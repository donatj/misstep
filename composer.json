{
	"name": "donatj/misstep",
	"type": "library",
	"description": "Simple Plain Text MySQL Schema Modeling Language",
	"keywords": [
	],
	"require": {
		"donatj/flags" : "1.*",
		"donatj/mysql-schema" : "^0.2.0",
		"php": ">=8.1"
	},
	"require-dev": {
		"donatj/drop": "*",
		"phpunit/phpunit": "3.7.*",
		"friendsofphp/php-cs-fixer": "^3.57",
		"squizlabs/php_codesniffer": "^3.7",
		"corpus/coding-standard": "^0.8.0",
		"phpstan/phpstan": "^2.0"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Jesse Donat",
			"email": "donatj@gmail.com",
			"homepage": "https://donatstudios.com",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"donatj\\Misstep\\": "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"bin": [
		"composer/bin/misstep"
	]
}
